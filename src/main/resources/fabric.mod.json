{
  "schemaVersion": 1,
  "id": "farmerscreate",
  "version": "${version}",
  "name": "FarmersCreate",
  "description": "Compat mod between Farmer's Delight cutting/cooking recipes and Create",
  "authors": [
    "ChefExperte"
  ],
  "contact": {
    "email": "aaron+minecraft@chefexperte.de",
    "sources": "https://github.com/ChefExperte/FarmersCreate",
    "issues": "https://github.com/ChefExperte/FarmersCreate/issues"
  },
  "license": "AGPL-3.0",
  "icon": "assets/farmerscreate/icon.png",
  "environment": "*",
  "entrypoints": {
    "fabric-datagen": [
      "de.chefexperte.farmersCreate.client.FarmersCreateDataGenerator"
    ],
    "client": [
      "de.chefexperte.farmersCreate.client.FarmersCreateClient"
    ],
    "main": [
      "de.chefexperte.farmersCreate.FarmersCreate"
    ]
  },
  "mixins": [
    "farmers-create.mixins.json",
    {
      "config": "farmers-create.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}",
    "farmersdelight": ">=1.21.11-3.4.1",
    "create": ">=1.21.11-6.0.8-2"
  }
}
